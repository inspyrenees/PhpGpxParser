{
  "name": "inspyrenees/phpgpxparser",
  "description": "GPX parsing and analysis toolkit in PHP, with elevation correction using the IGN API.",
  "type": "library",
  "version": "1.1.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": ["ign", "gpx", "elevation", "parser"],
  "authors": [
    {
      "name": "MINET Simon",
      "email": "simon@inspyrenees.fr"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/http-client": "^7.2",
    "league/geotools": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "PhpGpxParser\\": "src/PhpGpxParser"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpGpxParser\\Tests\\": "tests/"
    }
  }
}
